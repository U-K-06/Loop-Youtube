{
    "name":"LoopYoutube",
    "description":"Loop certain parts of vidoes, add custom conditions to them, Save them for future. Enjoy",
    "version":"0.6",
    "background": {
    "service_worker": "scripts/background_script.js"
  },
    "manifest_version":3,
    "permissions": ["storage"],
    "action": {
    "default_popup": "index.html"
},
  "content_scripts": [
    {
      "js": ["scripts/inject_custom_element.js"],
      "matches": [
        "https://*.youtube.com/*"
      ],
      "run_at": "document_idle"
    }
  ]

}